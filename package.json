    "typescript-eslint": "8.46.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@tsconfig/node-ts": "23.6.2",
    "@tsconfig/node24": "24.0.3",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
  }
