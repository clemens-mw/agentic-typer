    "prepare": "husky"
  },
  "type": "module",
  "author": "Clemens Pohle",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.1.30",
    "diff": "8.0.2",
    "eslint": "9.39.1",
